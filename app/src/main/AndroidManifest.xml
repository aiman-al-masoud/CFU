<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luxlunaris.cfu">
    <uses-permission android:name="android.permission.INTERNET" />



    <application

        android:networkSecurityConfig="@xml/enable_http_traffic_config"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CFU">



        <activity android:name=".frontEnd.activities.TablesListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".frontEnd.activities.TablesAnalyzerActivity"></activity>

        <activity android:name=".frontEnd.activities.SettingsActivity"></activity>


        <activity android:name=".frontEnd.activities.TableViewActivity"></activity>



    </application>

</manifest>